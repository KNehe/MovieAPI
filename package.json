{
  "name": "movieapi",
  "version": "1.0.0",
  "description": "Movie API",
  "main": "app.js",
  "scripts": {
    "start": "npm run prod",
    "server": "nodemon --exec babel-node  src/server.js",
    "dev": "cross-env NODE_ENV=development npm-run-all server",
    "prod": "cross-env NODE_ENV=production babel-node -- ./src/server.js",
    "test": "jest"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "author": "Nehemiah Kamolu",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "apollo-server-express": "^2.13.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-tools": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "rimraf": "^3.0.2",
    "sequelize": "^5.21.9",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "jest": "^26.0.1"
  }
}
